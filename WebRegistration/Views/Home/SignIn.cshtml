@using WebRegistration.models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, WebRegistration
@model WebRegistration.models.User
@{


    Layout = "_Layout";


}

<h1>SignIn</h1>
<div>
    <form asp-action="SignIn" method="post">
        <div>
            Name:<input asp-for="Name" />
        </div>
        <div>
            Email:<input asp-for="Email" />
        </div>
        <div>
            Password:<input asp-for="Password" />
        </div>
    </form>
</div>